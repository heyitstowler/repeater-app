{
  "name": "repeater-app-enhanced",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-scripts": "1.19.0",
    "@contentful/app-sdk": "^4.25.0",
    "@contentful/field-editor-single-line": "1.4.1",
    "@contentful/field-editor-tags": "1.4.15",
    "@contentful/field-editor-test-utils": "^0.9.0",
    "@contentful/f36-icons": "^4.28.2",
    "@contentful/f36-components": "4.66.0",
    "@contentful/f36-entity-list": "4.66.0",
    "@contentful/f36-tokens": "4.0.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "cross-env": "^7.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
