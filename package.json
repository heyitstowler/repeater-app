  "homepage": ".",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.3.1",
    "@types/node": "16.18.96",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/uuid": "^8.3.0",
    "typescript": "4.9.5"
  }
